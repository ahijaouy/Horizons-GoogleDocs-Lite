    "concurrently": "^3.5.0",
    "electron": "~1.6.2",
    "loglevel": "^1.4.1",
    "nodemon": "^1.11.0",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.6.0",
    "babel-eslint": "^6.1.0",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2"
  },
  "dependencies": {
    "axios": "^0.16.2",
